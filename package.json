{
  "name": "phoneserver",
  "version": "1.0.0",
  "description": "server which will run on android phone",
  "main": "index.js",
  "scripts": {
    "start": "cd build && node main",
    "compile": "node build.js && backpack build",
    "dev": "node build.js && backpack dev | pino -o 'short'",
    "debug": "node build.js && backpack dev --inspect-brk | pino -o 'short'",
    "test": "node build.js && mocha test/**/*.ts --exit"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "pino": "^4.17.6",
    "source-map-support": "^0.5.6",
    "swagger-express-middleware": "^1.0.0-alpha.12"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.23",
    "@types/chai": "^4.1.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/pino": "^4.16.1",
    "awesome-typescript-loader": "^5.2.0",
    "backpack-core": "^0.7.0",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.5.2",
    "mocha": "^5.2.0",
    "shelljs": "^0.8.2",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
